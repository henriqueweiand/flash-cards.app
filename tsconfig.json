{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@core/*": [
        "./src/core/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@services/*": [
        "./src/core/services/*"
      ],
      "@providers/*": [
        "./src/core/providers/*"
      ],
      "@domain/*": [
        "./src/core/domain/*"
      ],
      "@hooks/*": [
        "./src/core/hooks/*"
      ],
      "@context/*": [
        "./src/core/context/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ]
    }
  }
}